design myDesign is	
	
	device resistor is
		attr refprefix = "R";
		attr name = "";
		attr value = "10K";
		attr package = "R0207";
		attr tolerance = "10%";
		pin a = {1};
		pin b = {2};
	end device resistor;
	
	net +vcc;
	net -vcc;
	net zero_low;
	net one_high;
	net combined;

begin
	
	inst(1:0) myRes of resistor is
		refDes = "U2";
		this(0).a = +vcc;
		this(0).b = zero_low;
		this(1).a = one_high;
		this(1).b = -vcc;
	
	end inst myRes;
	
	zero_low = combined;
	one_high = combined;
	+vcc = -vcc;
	
end design myDesign;
