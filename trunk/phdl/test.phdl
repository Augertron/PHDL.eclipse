design myDesign is	
	
	device resistor is
		attr refprefix = "R";
		attr name = "";
		attr value = "10K";
		attr package = "R0207";
		attr tolerance = "10%";
		pin[3:0] a = {1,2,3,4};
		pin[3:0] b = {8,7,6,5};
	end;
	
	
	net[3:0] myNet3;
	net mynet;
	net mynet2;
	

begin
	
	inst(3:0) myRes of resistor is
		//tolerance = "5%";
		
		myRes(1).tolerance = "5%";
		myRes(3).tolerance = "1%";
		//myRes(3,4).tolerance = "2%";
		//myRes(3).tolerance = "3%";
		//tolerance = "4%";
		//myRes.tolerance = "5%";
		//myRes.tolerance = "10%";
		//myRes(5).tolerance = "4%";
		//myRes(1,2,4,5).tolerance = "5%";
		
		//myRes(1,3,4).name = "Brad";
		//newattr myRes(1,3,2).hometown = "Neenah, Wisconsin";
		//newattr myRes(1:2).bestfriend = "Nelson";
		//newattr myRes.name = "Brad";
		//a = myNet4[2,1] & myNet2 & myNet3;
		//a[1:2] = myNet4;
		
		//myRes(3:2).a[1:2] = myNet3[1,2];
		myRes(3:2).a[3,0] = mynet & mynet;
		myRes.b = open;
		//myRes(2,3).a[2,1] = open;
		myRes(1:0).a = <mynet>;
		myRes(2,3).a[2,1] = <mynet2>;
		
		
		//myRes(3:2).a[3,0] = myNet3[2:1];
